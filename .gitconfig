[color]
	ui = true
[alias]
	co = checkout
  br = branch
  st = status
[init]
	tmplatedir = .git_tmp
[diff]
  tool = nvimdiff
[difftool "nvimdiff"]
  cmd = "nvim -R -d -c \"wincmd l\" -d \"$LOCAL\" \"$REMOTE\""
[mergetool "nvimdiff"]
  cmd = "nvim -d -c \"4wincmd w | wincmd J\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"  \"$MERGED\""
[web]
    browser = open
[browser "chrome"]
    cmd = open -a 'Firefox.app'
